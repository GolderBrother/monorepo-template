{
  "name": "@golderbrother/package1",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@9.1.3",
  "description": "A simple Vue 3 component that utilizes the powerful features of the Monaco Editor for code editing.",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "GolderBrother",
    "email": "1204788939@qq.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
